@model Category

<div class="text-center">
    <h1>@ViewBag.OneCategory.Name</h1>
    <div class="row">
        <div class="col-6">
            <h3>Categories:</h3>
            <ul>
                @foreach (Association a in ViewBag.OneCategory.ProductsInCategory)
                {
                    <li>@a.Product.Name</li>
                }
            </ul>
        </div>
        <div class="col-6">
            <partial name="_AddProduct" />
        </div>
    </div>
</div>